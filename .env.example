# =================================================================
# CONFIGURAZIONE FORNITORI PUBBLICITÃ€ VIDEO
# =================================================================

# Google AdSense (Raccomandato per iniziare)
GOOGLE_ADS_ENABLED=false
GOOGLE_ADS_PUBLISHER_ID=
GOOGLE_ADS_TEST_MODE=true
GOOGLE_ADS_AUTO_ADS=true

# Unity Ads (Ideale per gaming)
UNITY_ADS_ENABLED=false
UNITY_ADS_GAME_ID=
UNITY_ADS_API_KEY=
UNITY_ADS_TEST_MODE=true

# Vungle Ads (Specializzato in video)
VUNGLE_ADS_ENABLED=false
VUNGLE_ADS_APP_ID=
VUNGLE_ADS_API_KEY=
VUNGLE_ADS_TEST_MODE=true

# IronSource Ads (Mediation platform)
IRONSOURCE_ADS_ENABLED=false
IRONSOURCE_ADS_APP_KEY=
IRONSOURCE_ADS_SECRET_KEY=
IRONSOURCE_ADS_TEST_MODE=true

# Configurazioni generali
AD_CACHE_DURATION=900
MAX_ADS_PER_PAGE=3
AD_TRACKING_ENABLED=true
AD_ANALYTICS_ENABLED=true

# Configurazioni per posizioni
AD_AUTO_ROTATION=true
AD_PERFORMANCE_THRESHOLD=0.02
AD_FILL_RATE_THRESHOLD=0.80
AD_ROTATION_INTERVAL=3600

# Geotargeting
AD_GEOTARGETING_ENABLED=false

# Device targeting
AD_MOBILE_FIRST=true
AD_DESKTOP_OPTIMIZED=true
AD_TABLET_SUPPORT=true

# Testing
AD_TESTING_ENABLED=true
AD_TEST_USERS=
AD_TEST_IPS=
AD_TEST_MODE_ADS_ONLY=true

# Compliance e Privacy
AD_GDPR_COMPLIANT=true
AD_CCPA_COMPLIANT=false
AD_CONSENT_REQUIRED=true
AD_COOKIE_CONSENT=true

# Analytics
AD_DETAILED_TRACKING=true
AD_REPORTING_INTERVAL=daily

# Failover
AD_FAILOVER_ENABLED=true
AD_TIMEOUT_SECONDS=3
AD_RETRY_ATTEMPTS=2

# =================================================================
# CONFIGURAZIONE SERVIZI VIDEO ADS AVANZATI
# =================================================================

# Google AdSense for Video
GOOGLE_ADSENSE_AD_CLIENT=
GOOGLE_ADSENSE_AD_SLOT=
GOOGLE_ADSENSE_AD_FORMAT=video
GOOGLE_ADSENSE_AD_THEME=dark
GOOGLE_ADSENSE_AD_LANGUAGE=it
GOOGLE_ADSENSE_AD_PLATFORM=mobile
GOOGLE_ADSENSE_TEST_MODE=false

# VAST (Video Ad Serving Template)
VAST_USER_AGENT="Globio-Video-Player/1.0"
VAST_TIMEOUT=10
VAST_CACHE_TTL=300

# VMAP (Video Multiple Ad Playlist)
VMAP_USER_AGENT="Globio-Video-Player/1.0"
VMAP_TIMEOUT=10
VMAP_CACHE_TTL=300

# Video Ads Settings
VIDEO_ADS_CACHE_ENABLED=true
VIDEO_ADS_CACHE_TTL=300
VIDEO_ADS_MAX_PER_VIDEO=3
VIDEO_ADS_SKIP_DELAY=5
VIDEO_ADS_FREQUENCY_CAP=1
